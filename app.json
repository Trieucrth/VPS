{
  "expo": {
    "name": "cobic-app",
    "slug": "cobic-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-cobic.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-cobic.png",
        "backgroundColor": "#8B5CF6"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-cobic.png"
    },
    "plugins": [
      "expo-router",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
